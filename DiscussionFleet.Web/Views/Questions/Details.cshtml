                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                             viewBox="0 0 24 24" fill="currentColor"
                             class="icon icon-tabler icons-tabler-filled icon-tabler-rosette">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none"/>
